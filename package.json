{
  "name": "eos-ecc",
  "version": "2.1.0",
  "description": "A universal JavaScript ECDSA packaged for EOS blockchain.",
  "main": "public/index.js",
  "engines": {
    "node": "^12.20.1 || >= 13.2"
  },
  "exports": {
    ".": {
      "import": "./public/index.mjs",
      "require": "./public/index.js"
    },
    "./public/": "./public/",
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "files": [
    "public",
    "private"
  ],
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "scripts": {
    "prepare": "npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:jsdoc": "jsdoc-md",
    "prepare:prettier": "prettier --write readme.md",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:api",
    "test:api": "coverage-node test/index.test.mjs",
    "test:eslint": "eslint .",
    "test:prettier": "prettier -c .",
    "prepublishOnly": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pur3miish/eos-ecc.git"
  },
  "author": "pur3miish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pur3miish/eos-ecc/issues"
  },
  "homepage": "https://github.com/pur3miish/eos-ecc#readme",
  "keywords": [
    "blockchain",
    "digital",
    "signature",
    "ecc",
    "elliptic",
    "curve",
    "cryptography",
    "bitcoin",
    "secp256k1",
    "WASM",
    "Javascript",
    "eos",
    "eosjs",
    "alternative"
  ],
  "devDependencies": {
    "coverage-node": "^5.0.1",
    "eslint": "^8.2.0",
    "eslint-config-env": "^23.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-compat": "^4.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jsdoc-md": "^11.0.2",
    "prettier": "^2.4.1",
    "test-director": "^7.0.0"
  },
  "dependencies": {
    "base58-js": "^1.0.0",
    "ripemd160-js": "^1.2.0",
    "universal-ecdsa": "^1.1.4"
  }
}
